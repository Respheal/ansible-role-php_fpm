; {{ template_destpath }}
; {{ ansible_managed }}

[core]
allow_url_fopen = {{ php_ini_allow_url_fopen }}
max_execution_time = {{ php_ini_max_execution_time }}
max_input_vars = {{ php_ini_max_input_vars }}
memory_limit = {{ php_ini_memory_limit }}
post_max_size = {{ php_ini_post_max_size }}
upload_max_filesize = {{ php_ini_upload_max_filesize }}
user_ini.cache_ttl = {{ php_ini_user_ini_cache_ttl }}
user_ini.filename = {{ php_ini_user_ini_filename }}

[opcache]
opcache.enable = {{ php_ini_opcache_enable }}
opcache.enable_cli = {{ php_ini_opcache_enable_cli }}
opcache.force_restart_timeout = {{ php_ini_opcache_force_restart_timeout }}
opcache.interned_strings_buffer = {{ php_ini_opcache_interned_strings_buffer }}
opcache.log_verbosity_level = {{ php_ini_opcache_log_verbosity_level }}
opcache.max_accelerated_files = {{ php_ini_opcache_max_accelerated_files }}
opcache.memory_consumption = {{ php_ini_opcache_memory_consumption }}
opcache.revalidate_freq = {{ php_ini_opcache_revalidate_freq }}
opcache.validate_permission = {{ php_ini_opcache_validate_permission }}
opcache.validate_root = {{ php_ini_opcache_validate_root }}
opcache.validate_timestamps = {{ php_ini_opcache_validate_timestamps }}

[zlib]
zlib.output_compression = {{ php_ini_zlib_output_compression }}
zlib.output_compression_level = {{ php_ini_zlib_output_compression_level }}
